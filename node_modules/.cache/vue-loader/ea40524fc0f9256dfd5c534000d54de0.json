name=\"language\" label=\"語言\">\n      <template>\n        <van-radio-group direction=\"horizontal\" v-model=\"language\">\n          <van-radio name=\"中文\">中文</van-radio>\n          <van-radio name=\"簡體\">簡體</van-radio>\n          <van-radio name=\"英文\">英文</van-radio>\n        </van-radio-group>\n      </template>\n    </van-field>\n    <van-field name=\"uploader\" label=\"封面上傳\">\n      <template #input>\n        <van-uploader v-model=\"img\" />\n      </template>\n    </van-field>\n    <van-field\n      readonly\n      clickable\n      name=\"datetimePicker\"\n      :value=\"`${date}`\"\n      label=\"出版日期\"\n      :placeholder=\"currentDate\"\n      @click=\"popupController(true)\"\n    />\n    <van-popup v-model=\"showPicker\" :show=\"showPicker\" position=\"bottom\">\n      <van-datetime-picker\n        title=\"出版日期\"\n        type=\"date\"\n        :min-date=\"publisherMinDate\"\n        confirm-button-text=\"確認\"\n        cancel-button-text=\"取消\"\n        v-model=\"date\"\n        @confirm=\"onConfirm\"\n        @cancel=\"popupController(false)\"\n      />\n    </van-popup>\n\n    <van-button type=\"primary\" class=\"sent-btn\" size=\"large\"\n      >提交書單</van-button\n    >\n  </div>\n</template>\n\n<script>\nimport { ref, reactive, toRefs } from '@vue/composition-api'\n\nexport default {\n  name: \"HelloWorld\",\n  props: {\n    msg: String,\n  },\n  setup() {\n    // let book:Book = reactive({\n    //   name: \"\",\n    //   author: \"\",\n    //   date: new Date(),\n    //   publisher: \"\",\n    //   language: \"\",\n    //   flipped: false,\n    //   img: []\n    // })\n\n    let publisherRange = reactive({\n      publisherMinDate: new Date(1950, 0, 1),\n    })\n\n    let showPicker = ref(false),\n        currentDate = ref(\"點擊選擇時間\")\n\n    let popupController = (state) => {\n      showPicker.value = state\n    }\n\n    let onConfirm = (time) => {\n      book.date = time;\n      let year = time.getFullYear(),\n          month = time.getMonth() + 1,\n          date = time.getDate()\n      currentDate.value = `${year}/${padLeftFormatted(month)}/${padLeftFormatted(date)}`\n      popupController(false)\n    }\n\n    let padLeftFormatted = (num) => {\n      return (num >= 10) ? `${num}` : `0${num}`;\n    }\n\n    return {\n      ...toRefs(publisherRange),\n      ...toRefs(book),\n      showPicker,\n      currentDate,\n      popupController,\n      onConfirm\n    }\n  }\n};\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\nh3 {\n  margin: 40px 0 0;\n}\nul {\n  list-style-type: none;\n  padding: 0;\n}\nli {\n  display: inline-block;\n  margin: 0 10px;\n}\na {\n  color: #42b983;\n}\n</style>\n"]}]}